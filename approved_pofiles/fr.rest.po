msgid "<var>CardinalThrough20(-B)</var> less than something means that we subtract <code><var>-B</var></code> from it."
msgstr "La différence entre un nombre et <var>CardinalThrough20(-B)</var> est le résultat de la soustraction de <code><var>-B</var></code> à ce nombre."

msgid "<var>CardinalThrough20(-B)</var> less than the product of <var>cardinalThrough20(M)</var> and a number <code>x</code>."
msgstr "La différence entre le produit de <var>cardinalThrough20(M)</var> par <code>x</code> et <var>CardinalThrough20(-B)</var>."

msgid "<var>CardinalThrough20(B)</var> minus something means that we subtract it from <code><var>B</var></code>."
msgstr "La différence entre <var>CardinalThrough20(B)</var> et un nombre est le résultat de la soustraction de ce nombre à <code><var>B</var></code>."

msgid "<var>CardinalThrough20(B)</var> minus the product of <var>cardinalThrough20(-M)</var> and a number <code>x</code>."
msgstr "La différence entre <var>CardinalThrough20(B)</var> et le produit de <var>cardinalThrough20(-M)</var> par <code>x</code>."

msgid "<var>CardinalThrough20(B)</var> more than something means that we add <code><var>B</var></code> to it."
msgstr "La somme de <var>CardinalThrough20(B)</var> et d'un nombre est le résultat de l'addition de <code><var>B</var></code> et de ce nombre."

msgid "<var>CardinalThrough20(B)</var> more than the product of <var>cardinalThrough20(M)</var> and a number <code>x</code>."
