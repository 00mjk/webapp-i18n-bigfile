msgid "So, <code><var>N * N</var> &lt; <var>Q</var> &lt; <var>(N + 1) * (N + 1)</var></code>."
msgstr "Zatem, <code><var>N * N</var> &lt; <var>Q</var> &lt; <var>(N + 1) * (N + 1)</var></code>."

msgid "So, <code>\\dfrac{<var>NUM_1</var>}{<var>DEN_1</var>} <var>SOLUTION</var> \\dfrac{<var>NUM_2</var>}{<var>DEN_2</var>}</code>."
msgstr "Zatem <code>\\dfrac{<var>NUM_1</var>}{<var>DEN_1</var>} <var>SOLUTION</var> \\dfrac{<var>NUM_2</var>}{<var>DEN_2</var>}</code>."

msgid "So, <code>\\sqrt{<var>NUM</var>} = \\sqrt{<var>SPLIT[0]</var>^2} = <var>SPLIT[0]</var></code>"
msgstr "Więc <code>\\sqrt{<var>NUM</var>} = \\sqrt{<var>SPLIT[0]</var>^2} = <var>SPLIT[0]</var></code>"

msgid "So, <code>x = <var>ABs</var></code>."
msgstr "Więc <code>x = <var>ABs</var></code>."

msgid "So, <code>x = <var>AC + ACrs</var></code>."
msgstr "Więc <code>x = <var>AC + ACrs</var></code>."

