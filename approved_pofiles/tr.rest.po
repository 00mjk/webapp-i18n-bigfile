msgstr "Buna göre, <code><var>ANGLE_B_LABEL</var> = 180^\\circ - <var>SUBANGLE_1</var></code>"

msgid "So, <code><var>ASKED_LABEL</var> = 2 \\cdot <var>GIVEN_LABEL</var></code>."
msgstr "Buna göre, <code><var>ASKED_LABEL</var> = 2 \\cdot <var>GIVEN_LABEL</var></code>."

msgid "So, <code><var>ASKED_LABEL</var> = \\frac{1}{2}<var>GIVEN_LABEL</var></code>."
msgstr "Buna göre,  <code><var>ASKED_LABEL</var> = \\frac{1}{2}<var>GIVEN_LABEL</var></code>."

msgid "So, <code><var>IDENT</var> = <var>EQUIV</var></code>"
msgstr "Buna göre, <code><var>IDENT</var> = <var>EQUIV</var></code>"

