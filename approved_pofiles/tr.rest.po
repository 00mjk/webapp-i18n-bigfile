msgstr "<code>x = {}</code><span class=\"entry short35\"></span> veya <code>x = {}</code><span class=\"entry short35\"></span>"

msgid "<code>x</code> = <span class=\"sol short50\"><var>X</var></span>"
msgstr "<code>x</code> = <span class=\"sol short50\"><var>X</var></span>"

msgid "<code>x</code> = <span class=\"sol\"><var>X_NUMER / X_DENOM</var></span>"
msgstr "<code>x</code> = <span class=\"sol\"><var>X_NUMER / X_DENOM</var></span>"

msgid "<code>x</code> = <span class=\"sol\"><var>X_NUMER2 / X_DENOM2</var></span>"
msgstr "<code>x</code> = <span class=\"sol\"><var>X_NUMER2 / X_DENOM2</var></span>"

msgid "<code>x^{1} = x</code>, no matter what <code>x</code> is."
msgstr "<code>x</code> ne olursa olsun, <code>x^{1} = x</code>."

msgid "<code>x_1</code> and <code>y_1</code> are the coordinates of the center of the ellipse."
msgstr "<code>x_1</code> ve <code>y_1</code>, elipsin merkezinin koordinatlarıdır."

